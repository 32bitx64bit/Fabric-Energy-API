{
	"schemaVersion": 1,
	"id": "fabric-energy-api",
	"version": "${version}",
	"name": "Fabric Energy API",
	"description": "A standardized energy API for Fabric mods.",
	"authors": [
		"Gavinx"
	],
	"contact": {
		"homepage": "",
		"sources": "https://github.com/32bitx64bit/Fabric-Energy-API"
	},
